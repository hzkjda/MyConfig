<pre>function aa_mod_parameters ()=20
{=20
    N=3D/dev/null;
    C=3D`tput op` O=3D$(echo -en "\n`tput setaf 2`&gt;&gt;&gt; `tput op`");
    for mod in $(cat /proc/modules|cut -d" " -f1);
    do
        md=3D/sys/module/$mod/parameters;
        [[ ! -d $md ]] &amp;&amp; continue;
        m=3D$mod;
        d=3D`modinfo -d $m 2&gt;$N | tr "\n" "\t"`;
        echo -en "$O$m$C";
        [[ ${#d} -gt 0 ]] &amp;&amp; echo -n " - $d";
        echo;
        for mc in $(cd $md; echo *);
        do
            de=3D`modinfo -p $mod 2&gt;$N | grep ^$mc 2&gt;$N|sed "s/^$mc=
=3D//" 2&gt;$N`;
            echo -en "\t$mc=3D`cat $md/$mc 2&gt;$N`";
            [[ ${#de} -gt 1 ]] &amp;&amp; echo -en " - $de";
            echo;
        done;
    done
}</pre>
