<pre style=3D"margin-top: 0; border-top-style:dashed; padding-top: 0.8em;">=
#!/bin/sh

tmp=3D${TMPDIR-/tmp}/pacman-disowned-$UID-$$
db=3D$tmp/db
fs=3D$tmp/fs

mkdir "$tmp"
trap  'rm -rf "$tmp"' EXIT

pacman -Qlq | sort -u &gt; "$db"

find /bin /etc /lib /sbin /usr \
  ! -name lost+found \
  \( -type d -printf '%p/\n' -o -print \) | sort &gt; "$fs"

comm -23 "$fs" "$db"
