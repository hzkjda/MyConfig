<pre style=3D"margin-top: 0; border-top-style:dashed; padding-top: 0.8em;">=
#!/bin/sh
exec tail -n +3 $0
# This file provides an easy way to add custom menu entries.&nbsp; Simply t=
ype the
# menu entries you want to add after this comment.&nbsp; Be careful not to =
change
# the 'exec tail' line above.

menuentry "Microsoft Windows 8.1" {
	echo "Loading Microsoft Windows 8.1..."
	insmod part_gpt
	insmod fat
	insmod search_fs_uuid
	insmod chain
	search --fs-uuid --no-floppy --set=3Droot --hint-bios=3Dhd0,gpt2 --hint-ef=
i=3Dhd0,gpt2 --hint-baremetal=3Dahci0,gpt2 763A-9CB6
	chainloader /EFI/Microsoft/Boot/bootmgfw.efi
}

menuentry "Microsoft Control Center" {
	echo "Loading Microsoft Control Center..."
	insmod part_gpt
	insmod fat
	insmod search_fs_uuid
	insmod chain
	search --fs-uuid --no-floppy --set=3Droot --hint-bios=3Dhd0,gpt2 --hint-ef=
i=3Dhd0,gpt2 --hint-baremetal=3Dahci0,gpt2 763A-9CB6
	chainloader /EFI/HP/boot/bootmgfw.efi
}

menuentry "System shutdown" {
	echo "System shutting down..."
	halt
}

menuentry "System restart" {
	echo "System rebooting..."
	reboot
}</pre>
