#!/bin/bash

count_of_int()
{
    if [ $1 -gt 9999 ]
    then
        let "place=3D5"         #5=E4=BD=8D=E6=95=B0
    elif [ $1 -gt 999 ]
    then
        let "place=3D4"
    elif [ $1 -gt 99 ]
    then
        let "place=3D3"
    elif [ $1 -gt 9 ]
    then
       let "place=3D2"
    else
        let "place=3D1"         #1=E4=BD=8D=E6=95=B0
    fi
   =20
    echo "The place of the $1 is $place."      #=E6=95=B4=E6=95=B0=E7=9A=84=
=E4=BD=8D=E6=95=B0
}=20

num_of_int()
{
    let "ten_thousand =3D $1/10000"           #=E6=95=B4=E6=95=B0=E7=9A=84=
=E6=95=B0=E4=BD=8D=E5=88=86=E8=A7=A3=E5=80=BC   =20
    let "thousand =3D$1/1000%10"
    let "hundred =3D $1/100%10"
    let "ten =3D $1%100/10"
    let "indiv =3D $1%10"

    if [ $ten_thousand -ne 0 ]
    then
       echo "$ten_thousand  $thousand  $hundred  $ten  $indiv"
    elif [ $thousand -ne 0 ]
    then
        echo "$thousand  $hundred  $ten  $indiv"
    elif [ $hundred -ne 0 ]
    then
        echo "$hundred  $ten  $indiv"
    elif [ $ten -ne 0 ]
    then
       echo "$ten  $indiv"
    else
       echo "$indiv"
    fi
}

show()
{
    echo "Please input the number(1-99999): "
    read num
  =20
    count_of_int $num
    num_of_int $num
}

